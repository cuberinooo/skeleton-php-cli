{
  "name": "codingcube/codingcube",
  "type": "project",
  "description": "design patterns",
  "license": "MIT",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "discard-changes": true,
    "platform": {
      "php": "8.1"
    }
  }
}
